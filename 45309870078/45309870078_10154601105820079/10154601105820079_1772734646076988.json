        }, 
        {
            "created_time": "2017-07-08T19:46:13+0000", 
            "id": "10154601105820079_281834232292227", 
            "message": "? \u062d\u062c\u064a\u0643 \u0645\u0627\u0628\u064a \u0631\u0628\u0637. \u063a\u064a\u0648\u062b\u064a \u0627\u0628\u0648 \u0627\u0644\u0645\u064a\u0648 \ud83d\ude02\ud83d\ude02"
        }, 
        {
            "created_time": "2017-07-08T19:46:47+0000", 
            "id": "10154601105820079_844730555683373", 
            "message": "\u0627\u0643\u0644 \u0639\u064a\u0631 \u0645\u0633\u062a\u0631 \u0628\u0627\u0643\u0627 \ud83d\ude02 \u0627\u0643\u0648\u0644 \u0645\u064a\u0648 \u0628\u0633 \u0645\u0627\u064a\u0633\u0645\u0648\u0646\u064a \u0645\u0633\u062a\u0631 \u063a\u0628\u064a \u0648\u0631\u0627\u0636\u064a \u0628\u064a\u0647\u0629\ud83d\ude02"
        }, 
        {
            "created_time": "2017-07-08T19:47:36+0000", 
            "id": "10154601105820079_249829158847990", 
            "message": "\u0637\u0627\u062d \u062d\u0638\u0643 \u0647\u0627\u064a \u0639\u0648\u062f \u0627\u0646\u062a. \u0627\u0644\u0639\u0644\u0645\u062a\u0646\u064a \u0648\u064a\u0633\u0645\u0648\u0643 \u0645\u0633\u062a\u0631 \u0628\u0627\u0643\u0627 \u0643\u0628\u0631 \u0639\u0644\u064a \u0645\u0639\u062a\u0632\u0629 \u0627\u062d\u0633\u0646 \u0645\u0646\u0643 \ud83d\ude02"
        }, 
        {
            "created_time": "2017-07-08T19:47:48+0000", 
            "id": "10154601105820079_224090854779279", 
            "message": "\u0627\u0646\u064a \u0633\u0648\u064a\u062a \u0648\u064a\u0627\u0647\u0629 \u0634\u064a.   \u0648\u0627\u0646\u064a \u0643\u0645\u062a \u0627\u0633\u0645\u064a\u0647\u0629 \u0643\u0627\u0644\u064a\u0633\u064a. \u0648\u0647\u064a\u0629 \u0633\u0648\u062a\u0646\u064a \u0645\u0633\u062a\u0631 \u0628\u0627\u0643\u0627. \u0648\u062b\u0627\u0646\u064a \u0634\u064a \u0627\u062d\u0633\u0646 \u0645\u0627 \u0627\u0646\u0643\u0644\u0628 \u0628\u0632\u0648\u0646\u0629 \u0628\u0627\u0644\u0644\u064a\u0644. \u0648\u062e\u0644\u064a\u0641\u0629. \u064a\u062f\u062e\u0644 \u0627\u0635\u0628\u0639\u0629 \u0628\u0635\u0631\u0645\u064a"
        }, 
        {
            "created_time": "2017-07-08T19:48:25+0000", 
            "id": "10154601105820079_1410364225717362", 
            "message": "\u0645\u0627\u0646\u0643\u0644\u0628\u062a \u0628\u0632\u0648\u0646\u0629 \u0628\u0633 \u0627\u0646\u062a \u0627\u0639\u0648\u0631 \u0648\u0645\u0627\u0642\u0631\u064a\u062a \u0639\u062f\u0644 \u0641 \u0644\u062a\u0645\u0636\u0631\u0637 \u064a\u0644\u0629 \u0627\u0634\u0637\u062d\ud83d\udc38\ud83d\udd95 \u0645\u0633\u062a\u0631 \u063a\u0628\u064a"
